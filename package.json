{
  "name": "huff",
  "version": "0.0.3",
  "displayName": "huff",
  "description": "VSCode syntax highlighting for huff programming language.",
  "keywords": [
    "ethereum",
    "huff"
  ],
  "publisher": "d1ll0n",
  "contributors": [
    "Dillon Kellar"
  ],
  "bugs": {
    "url": "https://github.com/d1ll0n/vscode-huff/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/d1ll0n/vscode-huff"
  },
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onView:huff.debugView",
    "onView:huff.debugMacro",
    "onCommand:huff.tools.startDebug",
    "onCommand:huff.tools.switchgenerator"
  ],
  "scripts": {
    "run": "node src/extension.js"
  },
  "main": "./src/extension.js",
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "huff-debug",
          "title": "Huff Debugger",
          "icon": "resources/huff.svg"
        }
      ]
    },
    "views": {
      "huff-debug": [
        {
          "type": "webview",
          "id": "huff.debugView",
          "name": "Functions"
        },
        {
          "type": "webview",
          "id": "huff.debugMacro",
          "name": "Macros"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "huff-debug",
        "contains": "huff-debug currently has a hard dependency on hevm, an implementation of the evm with a built in compiler, before you use this extension, please install hevm.",
        "when": "config"
      }
    ],
    "commands": [
      {
        "command": "huff.tools.switchgenerator",
        "title": "Huff: Generate MAIN() switch table from interface"
      },
      {
        "command": "calicoColors.addColor",
        "category": "Calico Colors",
        "title": "Add Color"
      },
      {
        "command": "calicoColors.clearColors",
        "category": "Calico Colors",
        "title": "Clear Colors",
        "icon": "$(clear-all)"
      }
    ],
    "snippets": [
      {
        "language": "huff",
        "path": "./src/snippets.json"
      }
    ],
    "languages": [
      {
        "id": "huff",
        "aliases": [
          "Huff",
          "huff"
        ],
        "extensions": [
          ".huff"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "huff",
        "scopeName": "source.huff",
        "path": "./syntaxes/huff.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/test-electron": "^2.1.3"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.0.0",
    "ethers": "^5.6.8",
    "huffc": "^0.0.25",
    "keccak": "^3.0.2"
  }
}
