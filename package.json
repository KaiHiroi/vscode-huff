{
  "name": "huff",
  "version": "0.0.1",
  "displayName": "huff",
  "description": "VSCode syntax highlighting for huff programming language.",
  "keywords": [
      "ethereum",
      "huff"
  ],
  "publisher": "d1ll0n",
  "contributors": [
    "Dillon Kellar"
  ],
  "bugs": {
    "url": "https://github.com/d1ll0n/vscode-huff/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/d1ll0n/vscode-huff"
  },
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "huff",
        "aliases": [
          "Huff",
          "huff"
        ],
        "extensions": [
          ".huff"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "huff",
        "scopeName": "source.huff",
        "path": "./syntaxes/huff.json"
      }
    ]
  }
}
