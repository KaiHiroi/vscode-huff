{
    "macro": {
        "prefix": ["macro", "#define-macro"],
        "body": ["#define macro ${1:name} = takes(${2:takes}) returns(${3:returns}) {\n\t${4:body}\n}"],
        "description": "Macro syntax shortcut"
    },
    "storage": {
        "prefix": ["storage", "new-storage"],
        "body": ["#define constant ${1:StorageName} = FREE_STORAGE_POINTER()"],
        "description": "Define a new storage variable with the new storage pointer"
    },
    "constant": {
        "prefix": ["constant"],
        "body": ["#define constant ${1:ConstantNem} = ${2:Value}"],
        "description": "Define a new constant"
    },
    "function": {
        "prefix": ["function"],
        "body": ["#define function ${1:name}(${2:args}) ${3:extensions} ${4:returns} (${5:returnType})"],
        "description": "Define a new interface function"
    },
